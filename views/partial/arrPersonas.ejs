<div class="container-fluid p-4">
    <h1 class="pb-4"><%= title %></h1>
    <table class="table table-striped table-hover">
    <thead>
    <tr>
    <th>Id</th>
    <th>nombre</th>
    <th>email</th>
    <th>fecha_registro</th>
    </tr>
    </thead>
    <tbody>
        <% arrPersonas.forEach((persona) => { %>
    <tr>
    <td><%= persona.id %></td>
    <td><%= persona.nombre %></td>
    <td><%= persona.email %></td>
    <td><%=
    persona.createdAt.toLocaleDateString('en-US')
    %>
    </td>
    </tr>
    <% }); %>
    </tbody>
    </table>
</div>